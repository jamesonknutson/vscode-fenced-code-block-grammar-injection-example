{
  "name": "nushell-grammar-injection",
  "displayName": "nushell-grammar-injection",
  "description": "",
  "version": "0.0.2",
  "publisher": "jamesonknutson",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mjbvz/vscode-fenced-code-block-grammar-injection-example.git"
  },
  "bugs": {
    "url": "https://github.com/mjbvz/vscode-fenced-code-block-grammar-injection-example/issues"
  },
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Other"
  ],
  "scripts": {
    "package-install": "vsce package && nu installer.nu"
  },
  "contributes": {
    "languages": [
      {
        "id": "nushell-injection"
      }
    ],
    "grammars": [
      {
        "language": "nushell-injection",
        "scopeName": "markdown.nushell.codeblock",
        "path": "./syntaxes/codeblock.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.nushell": "nushell"
        }
      }
    ]
  },
  "dependencies": {  }
}